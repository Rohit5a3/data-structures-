import numpy as np
n = int(input("enter no of elements:"))
arr = []
print(end='')
for i in range(0,n):
    ele = int(input())
    arr.append(ele)
print("enter indexes to be swapped:")
index1 = int(input("index1:"))
index2 = int(input("index2:"))
arr[index1], arr[index2]=arr[index2], arr[index1]
print(arr)
w=np.array(arr)
print(w)

